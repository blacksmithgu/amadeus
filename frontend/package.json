  },
  "homepage": "https://github.com/blacksmithgu/amadeus#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-node-resolve": "^8.1.0",
    "@types/jest": "^26.0.3",
    "@types/node": "^14.0.14",
    "jest": "^26.1.0",
    "rollup": "^2.7.6",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-scss": "^2.5.0",
    "rollup-plugin-terser": "^6.1.0",
